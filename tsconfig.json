{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "sourceMap":true
  },
    "include": [
    "src/**/*", "src/types.ts"
  ],
  "exclude": [
    "src/frontend/**/*",
    "node_modules",
    "dist"
  ]

}
